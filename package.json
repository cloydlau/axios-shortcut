{
  "name": "axios-shortcut",
  "PascalCasedName": "AxiosShortcut",
  "version": "0.1.1",
  "private": false,
  "packageManager": "pnpm@7.18.2",
  "description": "Shortcuts for axios.",
  "author": "cloydlau",
  "license": "MIT",
  "repository": "https://github.com/cloydlau/axios-shortcut",
  "keywords": [
    "axios",
    "download",
    "upload"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/axios-shortcut.umd.js",
      "import": "./dist/axios-shortcut.mjs"
    },
    "./*": "./*"
  },
  "main": "./dist/axios-shortcut.umd.js",
  "module": "./dist/axios-shortcut.mjs",
  "unpkg": "./dist/axios-shortcut.umd.js",
  "jsdelivr": "./dist/axios-shortcut.umd.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "docs"
  ],
  "scripts": {
    "dev": "vite --open",
    "serve": "vite preview",
    "doc": "vitepress dev --open /README",
    "build": "vite build",
    "release": "npx lint-staged && pnpm build && cl release"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.11.16",
    "@types/qs": "^6.9.7",
    "axios": "^1.2.1",
    "case-police": "^0.5.10",
    "husky": "^8.0.2",
    "lint-staged": "^13.1.0",
    "rome": "^11.0.0",
    "typescript": "^4.9.4",
    "vite": "^4.0.1",
    "vite-plugin-dts": "^1.7.1",
    "vitepress": "1.0.0-alpha.31"
  },
  "lint-staged": {
    "{src,__tests__}/**.*": "rome check",
    "*.md": "case-police --fix"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  }
}
