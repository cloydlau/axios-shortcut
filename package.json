  "devDependencies": {
    "@types/jest": "latest",
    "axios": "latest",
    "codecov": "latest",
    "jest": "latest",
    "qs": "latest",
    "rollup-plugin-peer-deps-external": "latest",
    "ts-jest": "latest",
    "tslib": "latest",
    "typescript": "latest",
    "vite": "latest"
  },
  "files": [
    "dist"
  ],
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  }
