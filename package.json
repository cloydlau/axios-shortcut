{
  "name": "axios-shortcut",
  "PascalCasedName": "AxiosShortcut",
  "version": "0.1.2",
  "private": false,
  "description": "Shortcuts for [Axios](https://github.com/axios/axios).",
  "author": "Cloyd Lau (https://github.com/cloydlau)",
  "license": "MIT",
  "repository": "https://github.com/cloydlau/axios-shortcut",
  "keywords": [
    "axios",
    "download",
    "upload"
  ],
  "exports": {
    ".": {
      "types": "./dist/axios-shortcut.d.ts",
      "import": "./dist/axios-shortcut.mjs",
      "require": "./dist/axios-shortcut.umd.js"
    },
    "./*": "./*"
  },
  "main": "./dist/axios-shortcut.umd.js",
  "module": "./dist/axios-shortcut.mjs",
  "unpkg": "./dist/axios-shortcut.umd.js",
  "jsdelivr": "./dist/axios-shortcut.umd.js",
  "types": "./dist/axios-shortcut.d.ts",
  "files": [
    "dist",
    "docs"
  ],
  "scripts": {
    "dev": "vite --open",
    "serve": "vite preview",
    "doc": "vitepress dev --open /README",
    "build": "vite build",
    "release": "esno ./scripts/release.mts",
    "pnpm:devPreinstall": "node ./scripts/devPreinstall.js",
    "preinstall": "node -e \"if (process.env.INIT_CWD === process.cwd()) { process.exit(1) }\" || npx only-allow pnpm",
    "postinstall": "node -e \"if (process.env.INIT_CWD === process.cwd()) { process.exit(1) }\" || npx simple-git-hooks"
  },
  "devDependencies": {
    "@antfu/eslint-config": "~0.42.1",
    "@commitlint/cli": "latest",
    "@commitlint/config-conventional": "latest",
    "@types/lodash-es": "latest",
    "@types/node": "latest",
    "@types/qs": "latest",
    "axios": "latest",
    "case-police": "latest",
    "cross-spawn": "latest",
    "del": "latest",
    "eslint": "latest",
    "esno": "latest",
    "kolorist": "latest",
    "lint-staged": "latest",
    "only-allow": "latest",
    "open": "latest",
    "prompts": "latest",
    "semver": "latest",
    "simple-git-hooks": "latest",
    "typescript": "latest",
    "vite": "latest",
    "vite-plugin-dts": "latest",
    "vitepress": "latest"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged --concurrent false",
    "commit-msg": "npx --no -- commitlint --edit ${1}"
  },
  "lint-staged": {
    "*.md": "case-police --fix",
    "*": "eslint --cache --ignore-pattern 'demo.ts' --fix"
  }
}
