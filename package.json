{
  "name": "axios-shortcut",
  "PascalCasedName": "AxiosShortcut",
  "version": "0.1.3",
  "private": false,
  "description": "Shortcuts for [Axios](https://github.com/axios/axios).",
  "author": {
    "name": "Cloyd Lau",
    "url": "https://github.com/cloydlau"
  },
  "license": "MIT",
  "repository": "https://github.com/cloydlau/axios-shortcut",
  "keywords": [
    "axios",
    "download",
    "upload"
  ],
  "exports": {
    ".": {
      "types": "./dist/axios-shortcut.d.ts",
      "import": "./dist/axios-shortcut.mjs",
      "require": "./dist/axios-shortcut.umd.js"
    },
    "./*": "./*"
  },
  "main": "./dist/axios-shortcut.umd.js",
  "module": "./dist/axios-shortcut.mjs",
  "unpkg": "./dist/axios-shortcut.umd.js",
  "jsdelivr": "./dist/axios-shortcut.umd.js",
  "types": "./dist/axios-shortcut.d.ts",
  "files": [
    "dist",
    "docs"
  ],
  "scripts": {
    "dev": "vite --open",
    "serve": "vite preview",
    "doc": "vitepress dev --open /README",
    "build": "vite build",
    "release": "esno ./scripts/release.mts",
    "lint": "eslint \"**/*.{vue,js,ts,mjs,mts,jsx,tsx,md,json}\"",
    "lint:fix": "eslint \"**/*.{vue,js,ts,mjs,mts,jsx,tsx,md,json}\" --fix",
    "preinstall": "node -e \"if (process.env.INIT_CWD === process.cwd()) { process.exit(1) }\" || npx only-allow pnpm",
    "postinstall": "node -e \"if (process.env.INIT_CWD === process.cwd()) { process.exit(1) }\" || npx simple-git-hooks"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.4",
    "@commitlint/cli": "^18.6.0",
    "@commitlint/config-conventional": "^18.6.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.11.16",
    "@types/qs": "^6.9.11",
    "axios": "^1.6.7",
    "case-police": "^0.6.1",
    "cross-spawn": "^7.0.3",
    "del": "^7.1.0",
    "eslint": "npm:eslint-ts-patch@8.56.0-0",
    "eslint-plugin-format": "^0.1.0",
    "eslint-ts-patch": "8.56.0-0",
    "esno": "^4.0.0",
    "kolorist": "^1.8.0",
    "lint-staged": "^15.2.2",
    "only-allow": "^1.2.1",
    "open": "^10.0.3",
    "prompts": "^2.4.2",
    "rollup-plugin-visualizer": "^5.12.0",
    "semver": "^7.6.0",
    "simple-git-hooks": "^2.9.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vite-plugin-dts": "^3.7.2",
    "vitepress": "1.0.0-rc.41"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged",
    "commit-msg": "npx --no -- commitlint --edit ${1}"
  },
  "lint-staged": {
    "*.md": "case-police --fix",
    "*.{vue,js,ts,mjs,mts,jsx,tsx,md,json}": "eslint --cache --fix"
  }
}
